{
  "name": "vite-base-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "serve": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --max-warnings 0 \"src/**/*.{vue,js,jsx}\" --cache --cache-location node_modules/.cache/eslint/",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,css,less,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{css,less,html,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "ant-design-vue": "3.1.0-rc.5",
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.0",
    "pinia": "^2.0.12",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.8.6",
    "vite-plugin-compression": "^0.5.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{less,scss,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
